---
import Page from "../../solutions/[solution].astro"
import { getStaticPaths as proxyGetStaticPaths } from "../../solutions/[solution].astro"
import { extractRouteLangCode } from "astro-i18n"

/* @ts-ignore */
export const getStaticPaths = (props) => proxyGetStaticPaths({
	...props,
	langCode: extractRouteLangCode(import.meta.url),
})

export const prerender = true

const { props } = Astro
---

<Page {...props} />